<Window x:Class="NugetHelperDNT.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NugetHelperDNT"
        mc:Ignorable="d"
        Title="DNT Nuget Switcher UI" Height="450" Width="800">
    <DockPanel>
        <StackPanel DockPanel.Dock="Top" Orientation="Horizontal">
            <StackPanel Orientation="Horizontal">
                <TextBox Name="TbTargetSolutionPath" Width="100"></TextBox>
                <Button>Open target file</Button>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <TextBox Name="TbSourceSolutionPath" Width="100"></TextBox>
                <Button>Open source file</Button>
            </StackPanel>
        </StackPanel>
        <DockPanel DockPanel.Dock="Left">
        </DockPanel>
        <DockPanel DockPanel.Dock="Right"></DockPanel>
    </DockPanel>
</Window>
